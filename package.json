{
  "author": "Minor Gordon",
  "bin": {
    "shacl2ts": "cli.js"
  },
  "dependencies": {
    "@rdfjs/prefix-map": "^0.1.2",
    "@rdfjs/term-map": "^2.0.2",
    "@rdfjs/term-set": "^2.0.3",
    "@rdfjs/types": "^1.1.0",
    "@sindresorhus/base62": "^0.1.0",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "@types/n3": "^1.21.1",
    "@types/rdfjs__prefix-map": "^0.1.5",
    "@types/rdfjs__term-map": "^2.0.10",
    "@types/rdfjs__term-set": "^2.0.9",
    "change-case": "^5.4.4",
    "cmd-ts": "^0.13.0",
    "n3": "^1.21.3",
    "pino": "^9.3.1",
    "purify-ts": "^2.1.0",
    "rdfjs-resource": "1.0.7",
    "reserved-identifiers": "^1.0.0",
    "shacl-ast": "2.0.6",
    "ts-invariant": "^0.10.3",
    "ts-morph": "^24.0.0",
    "typescript-memoize": "^1.1.1"
  },
  "description": "Generate TypeScript from SHACL shapes",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@kos-kit/sparql-builder": "2.0.90",
    "@tsconfig/strictest": "^2.0.5",
    "js-sha256": "^0.11.0",
    "purify-ts-helpers": "1.0.7",
    "rimraf": "3.0.2",
    "tsx": "^4.16.2",
    "typescript": "~5.6",
    "vitest": "^2.0.3"
  },
  "files": ["*.d.ts", "*.d.ts.map", "*.js", "*.js.map"],
  "license": "Apache-2.0",
  "main": "index.js",
  "name": "shacl2ts",
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minorg/shacl2ts"
  },
  "scripts": {
    "build": "tsc -b",
    "check": "biome check",
    "check:write": "biome check --write",
    "check:write:unsafe": "biome check --write --unsafe",
    "clean": "rimraf *.d.ts* *.js *.js.map __tests__/*.d.ts* __tests__/*.js __tests__/*.js.map tsconfig.tsbuildinfo",
    "link-dependencies": "npm link purify-ts-helpers rdfjs-resource shacl-ast",
    "test": "biome check && vitest",
    "test:watch": "vitest watch",
    "watch": "tsc -w --preserveWatchOutput"
  },
  "type": "module",
  "types": "index.d.ts",
  "version": "2.0.6"
}
