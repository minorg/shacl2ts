{
  "dependencies": {
    "@shaclmate/shacl-ast": "2.0.13",
    "@rdfjs/prefix-map": "^0.1.2",
    "@rdfjs/term-map": "^2.0.2",
    "@rdfjs/term-set": "^2.0.3",
    "@rdfjs/types": "^1.1.0",
    "@sindresorhus/base62": "^0.1.0",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "@types/n3": "^1.21.1",
    "@types/rdf-validate-shacl": "^0.4.7",
    "@types/rdfjs__prefix-map": "^0.1.5",
    "@types/rdfjs__term-map": "^2.0.10",
    "@types/rdfjs__term-set": "^2.0.9",
    "@types/toposort": "2.0.7",
    "change-case": "^5.4.4",
    "n3": "^1.21.3",
    "pino": "^9.1.0",
    "rdf-validate-shacl": "^0.5.6",
    "reserved-identifiers": "^1.0.0",
    "toposort": "2.0.2",
    "ts-invariant": "^0.10.3",
    "ts-morph": "^24.0.0",
    "typescript-memoize": "^1.1.1"
  },
  "devDependencies": {
    "@shaclmate/runtime": "2.0.13"
  },
  "files": [
    "*.d.ts",
    "*.js",
    "_ShapesGraphToAstTransformer/*.d.ts",
    "_ShapesGraphToAstTransformer/*.js",
    "ast/*.d.ts",
    "ast/*.js",
    "generators/*.d.ts",
    "generators/*.js",
    "generators/json/*.d.ts",
    "generators/json/*.js",
    "generators/ts/*.d.ts",
    "generators/ts/*.js",
    "generators/ts/_ObjectType/*.d.ts",
    "generators/ts/_ObjectType/*.js",
    "input/*.d.ts",
    "input/*.js",
    "vocabularies/*.d.ts",
    "vocabularies/*.js"
  ],
  "main": "index.js",
  "license": "Apache-2.0",
  "name": "@shaclmate/compiler",
  "scripts": {
    "build": "tsc -b",
    "check": "biome check",
    "check:write": "biome check --write",
    "check:write:unsafe": "biome check --write --unsafe",
    "clean": "rimraf -g **/*.d.ts* **/*.js **/*.js.map tsconfig.tsbuildinfo",
    "format": "biome format",
    "format:write": "biome format --write",
    "format:write:unsafe": "biome format --write --unsafe",
    "rebuild": "run-s clean build",
    "link-dependencies": "npm link purify-ts-helpers rdfjs-resource",
    "lint": "biome lint",
    "lint:write": "biome lint --write",
    "lint:write:unsafe": "biome lint --write --unsafe",
    "test": "biome check && vitest run",
    "test:coverage": "biome check && vitest run --coverage",
    "test:watch": "vitest watch",
    "unlink": "npm unlink -g @shaclmate/compiler",
    "watch": "tsc -w --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minorg/shaclmate"
  },
  "type": "module",
  "types": "index.d.ts",
  "version": "2.0.13"
}
